{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "plugins":[
        "security",
        "promise"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:security/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "semi":"off"
    }
}
